package com.example.sacs.ecosystem;

import org.json.JSONException;
import org.json.JSONObject;

public class Plants extends Data implements Interface {
    public Guardian(){
        setRelation(super.GUARDIAN);
        setPlantName("Cactus");
    }

    public Guardian(String plantName) {
        setRelation(super.GUARDIAN);
        setPlantName(plantName);
    }

    public Guardian(JSONObject json) throws JSONException {
        setRelation(super.GUARDIAN);
        mPlantName = json.getString(JSON_PLANT_NAME);
    }

    public String toString () {
        return "Guardian: " + mPlantName;
    }
}
